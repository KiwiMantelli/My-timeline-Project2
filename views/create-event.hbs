<div class="big-container">
  <form method="post" action="/general-create" class="event-form" enctype="multipart/form-data">
    <h3 class="form-title"> Create an event</h3>
    {{> catCreatePartials}}
    <div class="form-item-rating">
      <label for="event-rating" class="label-tags label hidden">Rating:</label>
      <span>
        <input type="radio" name="rating" id="str5" class="hidden" value="5">
        <input type="radio" name="rating" id="str4" class="hidden" value="4">
        <input type="radio" name="rating" id="str3" class="hidden" value="3">
        <input type="radio" name="rating" id="str2" class="hidden" value="2">
        <input type="radio" name="rating" id="str1" class="hidden" value="1">
      </span>
    </div>

    <input id="event-review" type="text" class="event-review input hidden" name="review"
      placeholder="give your opinion">

    <input id="event-author" type="text" class="event-author input hidden" name="author"
      placeholder="director or author">

    <div class="map hidden"></div>

    <input id="event-people" type="text" class="event-people input hidden" name="people"
      placeholder="ex: people who was with you">

    <button class="btn">Create!</button>
  </form>
</div>