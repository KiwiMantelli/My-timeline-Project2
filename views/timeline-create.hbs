<div class="big-container">
  <form method="post" action="/timeline-create" class="timeline-form" enctype="multipart/form-data">
    <h3 class="form-title"> Create Timeline</h3>
    <div class="form-item-wrapper">
      {{!-- <label for="timeline-name" class="label-name label">Timeline name:</label> --}}
      <input id="timeline-name" type="text" class="input-text input" name="name" required placeholder="title" >
    </div>
  
    <div class="form-item-wrapper">
      {{!-- <label for="timeline-cat" class="label-cat label">Category:</label> --}}
      <input id="timeline-cat" type="text" class="input-text input" name="category" required placeholder="ex: trips">
    </div>
   <div></div>
    <button class="btn">Create!</button>
  </form>
</div>